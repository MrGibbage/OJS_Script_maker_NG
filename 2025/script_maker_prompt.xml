        <jinjaTemplate>
            <filename>script_template.html.jinja</filename>
            <purpose>This is a jinja template file that will be rendered into an html file, suitable
                for an emcee to read at a closing ceremony. The file will have lists of teams, lists
                of awardees and lists of advancing teams. There is one closing ceremony, even if
                there are two divisions (and two OJS files). However, all of the awards are given
                out 'per-division', with the exception of the judges awards, which are given out
                'per-tournament'. For example, if a tournament is allocated 2 judges awards, those
                awards can be split among both divisions, or they can both go to a single division.
                All awards may be allocated less than the number permitted. For example, if Div 1 is
                permitted 2 Core Values awards, the user may select only one award, but that would
                trigger a warning just to remind the user.</purpose>
            <variables>
                <variable name="tournament_name"
                    refersTo="Meta table, keyName 'Tournament Long Name'" />
                <variable name="div1_list"
                    refersTo="OfficialTeamList table, columns 'Team Name' and 'Team Number', sorted by 'Team Number'."
                    instructions="This will be a list of all Div 1 teams. Include html tags with one team per line of output" />
                <variable name="div2_list"
                    refersTo="OfficialTeamList table, columns 'Team Name' and 'Team Number', sorted by 'Team Number'."
                    instructions="This will be a list of all Div 2 teams. Include html tags with one team per line of output" />
                <variable name="rg_div1_list"
                    refersTo="TournamentData table, 'Robot Game Rank' column. Also Meta Table, keyName Robot Game."
                    instructions="This will be a list of Div 1 teams with the highest Robot Game Rank (highest rank is the lowest number numerically. For example, first place will have a rank=1). Include html tags with one team per line of output. Include team number and team name." />
                <variable name="rg_div2_list"
                    refersTo="TournamentData table, 'Robot Game Rank' column. Also Meta Table, keyName Robot Game."
                    instructions="This will be a list of Div 2 teams with the highest Robot Game Rank (highest rank is the lowest number numerically. For example, first place will have a rank=1). Include html tags with one team per line of output. Include team number and team name." />
                <variable name="ip_div1_list"
                    refersTo="TournamentData table, Award column with value = 'Innovation Project * place', reverse sorted by place."
                    instructions="This will be a list of Div 1 teams winning an award for Innovation Project. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="ip_div2_list"
                    refersTo="TournamentData table, Award column with value = 'Innovation Project * place', reverse sorted by place."
                    instructions="This will be a list of Div 2 teams winning an award for Innovation Project. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="rd_div1_list"
                    refersTo="TournamentData table, Award column with value = 'Robot Design * place', reverse sorted by place."
                    instructions="This will be a list of Div 1 teams winning an award for Robot Design. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="rd_div2_list"
                    refersTo="TournamentData table, Award column with value = 'Robot Design * place', reverse sorted by place."
                    instructions="This will be a list of Div 2 teams winning an award for Robot Design. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="cv_div1_list"
                    refersTo="TournamentData table, Award column with value = 'Core Values * place', reverse sorted by place."
                    instructions="This will be a list of Div 1 teams winning an award for Core Values. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="cv_div2_list"
                    refersTo="TournamentData table, Award column with value = 'Core Values * place', reverse sorted by place."
                    instructions="This will be a list of Div 2 teams winning an award for Core Values. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="ja_list"
                    refersTo="TournamentData table, Award column with value = 'Judges Award *'"
                    instructions="This will be a list of Div 1 and Div 2 teams winning a judges award. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="champ_div1_list"
                    refersTo="TournamentData table, Award column with value = 'Champions * place', reverse sorted by place."
                    instructions="This will be a list of Div 1 teams winning a Champions award. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="champ_div2_list"
                    refersTo="TournamentData table, Award column with value = 'Champions * place', reverse sorted by place."
                    instructions="This will be a list of Div 2 teams winning a Champions award. Sort the list by place. For example, if there are awards for 2nd place and 1st place, list the 2nd place team, followed by the first place team. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="adv_div1_list"
                    refersTo="TournamentData table, Advance column with value = 'Yes'"
                    instructions="This will be a list of advancing Div 1 teams. Include html tags with one team per line of output. Include team number and team name." />
                <variable name="adv_div2_list"
                    refersTo="TournamentData table, Advance column with value = 'Yes'"
                    instructions="This will be a list of advancing Div 2 teams. Include html tags with one team per line of output. Include team number and team name." />
            </variables>
        </jinjaTemplate>
